{
    "plugins": [
        "jest"
    ],
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "no-underscore-dangle": [1, { "allow": true }],
        "no-console": "off",
        "class-methods-use-this": "off",
    },
    "env": {
        "jest": true,
        "browser": true,
        "jest/globals": true
    }
}